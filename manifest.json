{
  "name": "GoodData Extension Tool BETA",
  "version": "0.9107",
  "version_name": "0.9107",
  "icons": {
    "128": "gdlogo128_beta.png",
    "192": "gdlogo192_beta.png"
  },
  "description": "Makes your life easier when developing in GoodData Cloud BI platform",
  "options_page": "options.html",
  "background": {
    "persistent": false,
    "scripts": [
      "GDextension.js"
    ]
  },
  "page_action": {
    "default_icon": {
      "19": "icons/gd19_rebrand_gray.png",
      "38": "retina/icons/gd19_rebrand_gray.png"
    },
    "default_title": "Legen...wait for it GoodData is loading...dary!",
    "default_popup": "gdpopup.html"
  },
  "content_scripts": [
    {
      "matches": [
        "https://*/*"
      ],
      "js": [
        "gd_content_script.js"
      ],
      "css": [
        "gd_content.css"
      ]
    },
    {
      "matches": [
        "https://*/*/log*"
      ],
      "js": [
        "gd_cc_log_script.js",
        "cc_log_switching.js"
      ],
      "css": [
        "gd_cc_log.css"
      ]
    },
    {
      "matches": [
        "https://*/gdc/md/*/obj/*",
        "https://*/gdc/md/*/usedby2/*",
        "https://*/gdc/md/*/using2/*"
      ],
      "js": [
        "identifiers.js"
      ]
    },
    {
      "matches": [
        "https://*/dashboards/*"
      ],
      "js": [
        "gd_content_script_kpi.js"
      ]
    }
  ],
  "permissions": [
    "https://secure.gooddata.com/*",
    "activeTab",
    "storage"
  ],
  "optional_permissions": [
    "https://*/*",
    "tabs",
    "contextMenus",
    "notifications"
  ],
  "web_accessible_resources": [
    "cc_log_switching.js",
    "identifiers_inject.js"
  ],
  "manifest_version": 2
}